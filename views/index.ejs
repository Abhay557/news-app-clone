<%- include('layout', { body: `
    <section>
      <h2>Top Headlines</h2>
      <ul>
        <% articles.forEach(article => { %>
          <li>
            <h3><a href="<%= articles.url %>" target="_blank"><%= article.title %></a></h3>
            <p><%= article.description || 'No description available' %></p>
          </li>
        <% }); %>
      </ul>
    </section>
    ` }) %>
    